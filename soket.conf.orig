[program:soketi]
process_name=%(program_name)s_%(process_num)02d
command=soketi start --config=/home/vagrant/sites/server/soketi-config.json
autostart=true
autorestart=true
stopasgroup=true
killasgroup=true
user=root
numprocs=1
redirect_stderr=true
stdout_logfile=/var/log/supervisor/soketi.log
stopwaitsecs=60
stopsignal=sigint
minfds=10240
