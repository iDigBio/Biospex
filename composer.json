{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laracasts/utilities": "^3.0",
    "laravel/framework": "^v12.19.3",
    "laravel/tinker": "^v2.9.0",
    "laravel/ui": "^v4.5.1",
    "league/csv": "^9.15",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/oauth2-client": "^2.7",
    "mongodb/laravel-mongodb": "^5.1.1",
    "neitanod/forceutf8": "^2.0",
    "pda/pheanstalk": "^v7.0.1",
    "spiritix/lada-cache": "dev-l12-compatibility",
    "symfony/http-client": "^7.0",
    "symfony/mailgun-mailer": "^7.0",
    "tio/laravel": "^v1.23",
    "yajra/laravel-datatables-oracle": "^v12.4.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^v3.0.0",
    "deployer/deployer": "^7.5",
    "spatie/laravel-ignition": "^2.5.1",
    "fakerphp/faker": "^v1.23.0",
    "mockery/mockery": "^1.6.11",
    "nunomaduro/collision": "^v8.1.1",
    "phpunit/phpunit": "^12.2.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/laravel-shift/lada-cache.git"
    }
  ],
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  }
}
