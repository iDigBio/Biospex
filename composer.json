{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.2.*",
    "doctrine/dbal": "~2.5.2",
    "cartalyst/sentry": "dev-feature/laravel-5",
    "laravelcollective/html": "5.2.*",
    "vespakoen/menu": "3.*",
    "mcamara/laravel-localization": "1.1.*",
    "felixkiss/uniquewith-validator": "2.0.3",
    "cviebrock/eloquent-sluggable": "3.1.*",
    "cviebrock/image-validator": "2.0.*",
    "davejamesmiller/laravel-breadcrumbs": "~3.0",
    "pda/pheanstalk": "~3.0",
    "msurguy/honeypot": "dev-master",
    "ramsey/uuid": "3.1.*",
    "guzzlehttp/guzzle": "6.1.*",
    "jenssegers/mongodb": "2.2.*",
    "codesleeve/laravel-stapler": "dev-master",
    "league/csv": "8.0.*",
    "neitanod/forceutf8": "1.4.*",
    "dingo/api": "1.0.*@dev"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
    "barryvdh/laravel-debugbar": "2.1.*",
    "barryvdh/laravel-ide-helper": "2.1.*",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Biospex\\": "app/"
    },
    "files": ["app/Http/helpers.php", "app/Http/navigation.php"]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "minimum-stability": "dev"
  }
}
