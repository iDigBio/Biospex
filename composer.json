{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "anhskohbo/no-captcha": "^3.0",
    "cviebrock/eloquent-sluggable": "^4.8",
    "czim/laravel-paperclip": "^2.7",
    "dingo/api": "^2.1",
    "doctrine/dbal": "^2.7",
    "felixkiss/uniquewith-validator": "^3.2",
    "fideloper/proxy": "~4.0",
    "jenssegers/mongodb": "^3.5",
    "laracasts/utilities": "^3.0",
    "laravel/framework": "5.8.*",
    "laravel/nova": "~2.3.0",
    "laravel/passport": "^7.2",
    "laravel/telescope": "^2.0",
    "laravel/tinker": "~1.0",
    "league/csv": "^9.2",
    "league/oauth2-client": "^2.3",
    "mongodb/mongodb": "^1.4",
    "neitanod/forceutf8": "~2.0",
    "pda/pheanstalk": "^4.0",
    "predis/predis": "^1.1",
    "ramsey/uuid": "^3.7",
    "spiritix/lada-cache": "^3.0",
    "thiagoalessio/tesseract_ocr": "^2.6"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "cloudstudio/resource-generator": "^1.2",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
