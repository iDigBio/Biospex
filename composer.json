{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.2.*",
    "doctrine/dbal": "~2.5.2",
    "laravelcollective/html": "5.2.*",
    "mcamara/laravel-localization": "1.1.*",
    "felixkiss/uniquewith-validator": "2.*",
    "cviebrock/eloquent-sluggable": "3.1.*",
    "cviebrock/image-validator": "2.0.*",
    "davejamesmiller/laravel-breadcrumbs": "~3.0",
    "pda/pheanstalk": "~3.0",
    "msurguy/honeypot": "dev-master",
    "ramsey/uuid": "3.1.*",
    "guzzlehttp/guzzle": "6.1.*",
    "jenssegers/mongodb": "2.3.*",
    "codesleeve/laravel-stapler": "dev-master",
    "league/csv": "8.0.*",
    "neitanod/forceutf8": "1.4.*",
    "vetruvet/laravel-phpredis": "1.*",
    "unisharp/laravel-ckeditor": "^4.5",
    "barryvdh/laravel-translation-manager": "^0.2.2"
  },
  "require-dev": {
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
    "barryvdh/laravel-debugbar": "2.1.*",
    "barryvdh/laravel-ide-helper": "2.1.*",
    "fzaninotto/faker": "~1.5",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "behat/behat": "dev-master",
    "behat/mink": "dev-master",
    "behat/mink-extension": "dev-master",
    "laracasts/behat-laravel-extension": "^1.0",
    "codeception/codeception": "*",
    "xethron/migrations-generator": "dev-l5",
    "way/generators": "dev-feature/laravel-five-stable",
    "sven/artisan-view": "^1.0"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:jamisonvalenta/Laravel-4-Generators.git"
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": ["app/Http/helpers.php", "app/Http/navigation.php"]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "minimum-stability": "stable"
  }
}
