{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mongodb": "*",
    "anhskohbo/no-captcha": "^3.0",
    "aws/aws-sdk-php": "~3.0",
    "axn/laravel-pk-int-to-bigint": "^2.2",
    "cviebrock/eloquent-sluggable": "^10.0",
    "czim/laravel-paperclip": "^5.0",
    "doctrine/dbal": "^3.0",
    "graham-campbell/markdown": "^15.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laracasts/utilities": "^3.2",
    "laravel/framework": "^10.0",
    "laravel/nova": "4.27.13",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^4.0",
    "league/csv": "^9.6",
    "league/flysystem-aws-s3-v3": "^3.0",
    "league/fractal": "^0.20",
    "league/oauth2-client": "^2.6",
    "mongodb/laravel-mongodb": "^4.0",
    "neitanod/forceutf8": "~2.0",
    "outl1ne/nova-sortable": "^3.4",
    "pda/pheanstalk": "^4.0",
    "pusher/pusher-php-server": "^7.0",
    "ramsey/uuid": "^4.1",
    "rennokki/laravel-sns-events": "^7.2",
    "spatie/eloquent-sortable": "^4.0",
    "spiritix/lada-cache": "^5.1",
    "thiagoalessio/tesseract_ocr": "^2.9",
    "tio/laravel": "^1.10"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "barryvdh/laravel-ide-helper": "^2.8",
    "beyondcode/laravel-dump-server": "^1.4",
    "fakerphp/faker": "^1.9.1",
    "kitloong/laravel-migrations-generator": "^6.11",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^7.0",
    "pestphp/pest": "^2.24",
    "pestphp/pest-plugin-faker": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.2",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.0"
  },
  "repositories": {
    "nova": {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan nova:publish"
    ]
  }
}
