{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "doctrine/dbal": "~2.4.4",
    "cartalyst/sentry": "dev-feature/laravel-5",
    "laravelcollective/html": "~5.0",
    "vespakoen/menu": "3.*",
    "mcamara/laravel-localization": "1.0.*",
    "felixkiss/uniquewith-validator": "2.*",
    "cviebrock/eloquent-sluggable": ">=3.0.0-alpha",
    "cviebrock/image-validator": "2.0.*@alpha",
    "davejamesmiller/laravel-breadcrumbs": "~3.0",
    "pda/pheanstalk": "~3.0",
    "laravelcollective/html": "~5.0",
    "msurguy/honeypot": "dev-master",
    "maatwebsite/excel": "~2.0.0",
    "rhumsaa/uuid": "~2.8",
    "guzzlehttp/guzzle": "~5.2",
    "jenssegers/mongodb": "~2.1",
    "codesleeve/laravel-stapler": "dev-master",
    "laravelcollective/annotations": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Biospex\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
