{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "4.2.*",
    "doctrine/dbal": "~2.4",
    "jenssegers/mongodb": "*",
    "vespakoen/menu": "2.*",
    "mcamara/laravel-localization": "0.15.*",
    "cartalyst/sentry": "2.1.*",
    "felixkiss/uniquewith-validator": "1.0.*",
    "codesleeve/laravel-stapler": "1.0.*",
    "codesleeve/asset-pipeline": "dev-master",
    "cviebrock/eloquent-sluggable": "2.*",
    "cviebrock/image-validator": "1.0.*",
    "davejamesmiller/laravel-breadcrumbs": "~2.2.2",
    "pda/pheanstalk": "~2.0",
    "msurguy/honeypot": "dev-master",
    "ramsey/uuid": "~2.8",
    "guzzlehttp/guzzle": "~5.0",
    "league/csv": "^7.1",
    "neitanod/forceutf8": "dev-master"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "1.*",
    "barryvdh/laravel-ide-helper": "1.*",
    "raveren/kint": "dev-master",
    "behat/behat": "*@stable",
    "behat/mink": "*",
    "behat/mink-extension": "*",
    "behat/mink-goutte-driver": "*",
    "phpspec/phpspec": "2.2.*",
    "benconstable/phpspec-laravel": "~1.6.0",
    "xethron/migrations-generator": "dev-master"
  },
  "autoload": {
    "classmap": [
      "app/commands",
      "app/controllers",
      "app/models",
      "app/database/migrations",
      "app/database/seeds",
      "app/tests/TestCase.php"
    ],
    "psr-4": {
      "Biospex\\": "app/Biospex"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize --force"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "bin-dir": "vendor/bin/"
  },
  "minimum-stability": "stable"
}
